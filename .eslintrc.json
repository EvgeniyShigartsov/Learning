{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi":"off",
        "max-len": ["error", { "code": 130 }],
        "import/extensions": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "linebreak-style":"off"
    }
}
